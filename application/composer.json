{
  "name": "silinternational/email-service",
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json":"*",
    "aws/aws-sdk-php": "^3.136",
    "fillup/fake-bower-assets": "2.0.9",
    "silinternational/php-env": "^2.0.1 || ^3.0",
    "yiisoft/yii2": "~2.0.25",
    "silinternational/psr3-adapters": "^1.1 || ^2.0 || ^3.0",
    "yiisoft/yii2-gii": "*",
    "silinternational/yii2-json-log-targets": "^1.1",
    "silinternational/yii2-email-log-target": "^1.0",
    "yiisoft/yii2-swiftmailer": "^2.0"
  },
  "require-dev": {
    "codeception/codeception": ">=2.2.7",
    "codeception/specify": "^2.0",
    "codeception/verify": "^2.0",
    "codeception/module-asserts":  "^3.0",
    "codeception/module-yii2":  "^1.1.7",
    "roave/security-advisories": "dev-master",
    "silinternational/yii2-codeception": "^1.1.0",
    "yiisoft/yii2-symfonymailer": "^2.0",
    "codeception/module-rest": "^3.3",
    "codeception/module-phpbrowser": "^3.0",
    "friendsofphp/php-cs-fixer": "^3.39"
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
